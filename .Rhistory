m <- 29
n <- m + 1
q <- rep(10^4, n)  # 0-1 sequences: rep(10^6, n)
phi <- rep(0, n^2)
for (i in 1:n) {
for (j in 1:n) {
phi[(i - 1) * n + j] <- dnorm((i - j) / 3)  # 0-1 sequences: devide by 3
}
}
phi <- matrix(phi, ncol=n)
for (i in 1:n) {
phi[, i] <- sum(phi[, i]^2)^(-1/2) * phi[, i]
}
S <- t(phi) %*% phi
time <- proc.time()
L <- t(q * S) * q
proc.time() - time
time <- proc.time()
edc <- eigen(L)
lambda <- edc$values
lambda[lambda < 10^(-9)] <- 0
mean <- sum(lambda / (1 + lambda))
eigenvectors <- edc$vectors
lambda2 <- rep(mean / n / (1 - mean / n), n)
eigenvectors2 <- diag(rep(1, n))
proc.time() - time
